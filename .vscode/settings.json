{
    "[html][django-html]": {
        "editor.defaultFormatter": "monosans.djlint",
        "editor.formatOnSave": true,
    },
    "[python]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    "[shellscript]": {
        "files.eol": "\n",
        "editor.defaultColorDecorators": "never"
    },
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "files.eol": "\n",
}
